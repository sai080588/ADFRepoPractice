{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Revision121"
		},
		"LS_Ingest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Ingest'"
		},
		"LS_Ingest_Dest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Ingest_Dest'"
		},
		"LS_Src_Github_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Src_Github'"
		},
		"LS_Ingest_Dest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destsa12.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Revision121_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Ingest_connectionString": {
						"value": "[parameters('LS_Ingest_connectionString')]"
					},
					"LS_Ingest_Dest_accountKey": {
						"value": "[parameters('LS_Ingest_Dest_accountKey')]"
					},
					"LS_Src_Github_connectionString": {
						"value": "[parameters('LS_Src_Github_connectionString')]"
					},
					"LS_Ingest_Dest_properties_typeProperties_url": {
						"value": "[parameters('LS_Ingest_Dest_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}