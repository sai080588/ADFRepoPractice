{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Revision121"
		},
		"LS_Ingest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Ingest'"
		},
		"LS_Ingest_Dest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Ingest_Dest'"
		},
		"LS_Src_Github_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Src_Github'"
		},
		"LS_Ingest_Dest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destsa12.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
